{
  "name": "@elkdex/faas",
  "version": "1.0.0",
  "description": "Elk Finance FaaS contracts",
  "author": "Elk Finance",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && npm run typechain",
    "compile:local": "TS_NODE_TRANSPILE_ONLY=true hardhat --config hardhat.config.local.ts compile",
    "coverage": "hardhat coverage",
    "lint": "npm run lint:sol && npm run lint:ts && npm run prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"./contracts/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "start:local": "hardhat --config hardhat.config.local.ts node",
    "test": "hardhat --config hardhat.config.local.ts test",
    "typechain": "TS_NODE_TRANSPILE_ONLY=true hardhat typechain --config hardhat.config.local.ts"
  },
  "dependencies": {
    "@elkdex/avax-exchange-contracts": "^1.0.0",
    "@openzeppelin/contracts": "^4.8.0",
    "@tenderly/hardhat-tenderly": "^1.0.6",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=16.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.17.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.4",
    "ts-node": ">=8.0.0",
    "typechain": "^8.2.0",
    "typescript": ">=4.5.0"
  }
}
